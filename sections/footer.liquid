<footer id="footer">
  <div class="footer-row-01">
    <h3>Stay In Touch!</h3>
    <div class="instafeed--wrapper">
      <div id="insta-feed"></div>
    </div>
  </div>
  <div class="footer-row-02">
    <div class="footer-row-02__col-01">
      <svg
        version="1.1"
        id="footer-logo"
        xmlns="http://www.w3.org/2000/svg"
        x="0"
        y="0"
        viewBox="0 0 202.64 104.48"
        xml:space="preserve"
      >
        <style>
          .st0 {
            fill: #fdc20f;
          }
        </style>
        <path
          class="st0"
          d="M138.26 94.87c-25.13-.19-39.08-23.21-66.99-30.15 16.53-9.33 35.54-20.46 35.69-40.6C107.08 9.71 94.41.17 80.09.06 65.31-.06 54.83 9.28 54.7 26.19l12.62.04c-.1-11.46 4.52-20.16 12.46-20.09 6.28.05 12.73 6.82 12.64 17.41-.12 15.44-16.29 26.49-38.57 40.73l-.07 9.7c35.63 3.26 51.71 30.26 84.36 30.51 31.52.24 53.19-30.88 53.19-30.88l-9.23-7.76s-18.2 29.22-43.84 29.02z"
        />
        <path
          class="st0"
          d="M140.15 53.03c10.03.08 14.54-5.29 16.33-8.43-1.41 17.91-7.49 29.62-18.86 29.53-11.85-.09-19.16-18.56-19.16-18.56l-10.39 8.85s11.36 16.96 29.88 17.11c16 .12 35.71-13.73 34.39-47.33C171.49 12.81 158.8.14 141.31 0c-14.47-.11-28.42 10.04-27.76 26.6.69 17.66 14.3 26.34 26.6 26.43zm.99-47.08c10.32.08 14.28 10.3 15.31 21.05.35 9.4-2.25 14.95-7.01 17.55-1.61.82-3.52 1.43-5.79 1.68-.05.01-.1.01-.15.02-.77.08-1.58.12-2.44.12-9.39-.07-13.43-11.71-13.75-19.71-.62-11.23 6.05-20.77 13.83-20.71zM45.41 67.01l-30.56.02-.01-25.64 26.91-.01-.01-6.88-26.91.01V11.46l30.56-.01V4.1L0 4.12l.03 70.26 45.39.08zM182.71 7.38h7.06l.01 19.75h5.75l-.01-19.75 7.12-.01V4.46h-19.93zM196.45 43.27l-7.81.01v-9.3l-5.94.01.01 22.67 5.94-.01-.01-10.73h7.81v10.73h6.03l-.01-22.67h-6.03z"
        />
      </svg>
    </div>
    <div class="footer-row-02__col-02">

      <div class="newsletter">
        <h3>NATURAL CONFIDENCE</h3>
        <p>
          Subscribe and receive the latest East 29th updates straight to your
          inbox.
        </p>
        {% form 'customer' %}
        {{ form.errors | default_errors }}
        {% if form.posted_successfully? %}
        <div class="newsletter--form">
          <div class="note form-success">
            Thanks for signing up, please check your email to confirm your
            subscription.
          </div>
        </div>
        {% else %}
        <input type="hidden" name="contact[tags]" value="newsletter" />
        <div class="newsletter--form">
          <div class="input-group">
            <input
              type="email"
              value="{% if customer %}{{ customer.email }}{% endif %}"
              placeholder="Email"
              name="contact[email]"
              id="Email"
              class="input-group-field newsletter__input"
              autocorrect="off"
              autocapitalize="off"
            />
            <span class="input-group-btn">
              <button
                id="Subscribe"
                type="submit"
                class="button-main"
                aria-label="Newsletter sign-up"
              >
                SIGN ME UP
              </button>
            </span>
          </div>
        </div>
        {% endif %} {% endform %}
      </div>
    </div>
    <div class="footer-row-02__col-03">
      {% include 'footer-menu' %}
      <a href="/">
        <i class="fab fa-instagram-square fa-lg"></i>
      </a>
    </div>
  </div>
  <div class="footer-row-03">
    <div class="copyright--wrapper">
      <div class="footer-copyright">
        <span>&copy; {{ 'now' | date: '%Y' }} East 29th</span>
      </div>
    </div>
    <div class="sub-footer-menu--wrapper">
      {% include 'sub-footer-menu' %}
    </div>
  </div>
</footer>
